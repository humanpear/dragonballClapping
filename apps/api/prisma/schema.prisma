generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model MatchEvent {
  id        String   @id @default(cuid())
  matchId   String
  turnIndex Int
  type      String
  payload   Json
  createdAt DateTime @default(now())

  @@index([matchId, turnIndex])
}
