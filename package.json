{
  "name": "dragonball-clapping-monorepo",
  "private": true,
  "version": "0.2.0",
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "dev": "pnpm --parallel --filter @dragonball/web --filter @dragonball/api dev",
    "build": "npm run build --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "test": "npm run test --workspaces --if-present"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
